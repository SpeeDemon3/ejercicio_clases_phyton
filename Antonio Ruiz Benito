class Vehiculo:
    color = "rojo"
    ruedas = 4
    puertas = 5
    print(color, ruedas, puertas)
    
class Coche(Vehiculo):
    velocidad = 120
    cilindrada = 300
    print(velocidad, cilindrada)
    
c = Coche()
print(c)
